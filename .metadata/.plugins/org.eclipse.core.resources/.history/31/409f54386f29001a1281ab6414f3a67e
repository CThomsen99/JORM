package jormCore;

import java.sql.SQLException;
import java.util.List;
import jormCore.Tracing.*;
import jormCore.DBConnection.*;

public class ObjectSpace {

	private DatabaseConnection dbsession; 
	
	public ObjectSpace(JormApplication application) throws SQLException
	{
		PersistentTypeHelper.registerTypes(classList);
		Tracer.init(level);
		SQLiteConnection conn = new SQLiteConnection(connectionSting);
	}
	public DatabaseConnection getSession()
	{
		return dbsession;
	}
}
